('002', 'Ayam Geprek', 17000),
('003', 'Es Jeruk', 5000),
('004', 'Es Teh', 4000);
('O1', 'Ongkir', 28);

ALTER TABLE `tb_produk` ADD PRIMARY KEY (`id`);