CREATE TABLE IF NOT EXISTS `tb_produk` (
  `id` varchar(5) NOT NULL,
  `name` varchar(255) NOT NULL,
  `harga` int(11) NOT NULL,
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

INSERT INTO `tb_produk` (`id`, `name`, `harga`) VALUES
